(defun palindrome-p (string)
  (equal string (reverse string)))

(defun almost-palindrome-p (string)
  (let* ((k (floor (length string) 2))
         (s (coerce string 'list))
         (left (take k s))
         (right (take k (reverse s)))
         (bs (mapcar #'char/= left right))
         (i (position t bs)))
    (when (= (count t bs) 1)
      i)))
         
(defun solve (a)
  (let ((n (length a)))
    (cond ((and (evenp n) (palindrome-p a))
           (* 25 n))
          ((and (oddp n) (palindrome-p a))
           (* 25 (1- n)))
          ((almost-palindrome-p a)
           (+ (* 25 (- n 2))
              24 24))
          (t
           (* 25 n)))))
                
(defun main ()
  (let ((a (read-line)))
